pipeline {
    agent { 
		docker { 
			image 'maven:latest' 
		}
	}
    stages {
        stage('005 - Recording Test Results and Artifacts/02 - Java Unit Tests') {
            steps {
                sh "mvn clean install -f '005 - Recording Test Results and Artifacts/02 - Java Unit Tests/pom.xml'"
            }
        }
    }
}
